<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.drsystem.controller.DashboardController">

    <HBox alignment="CENTER">
        <Button text="Logout" onAction="#logout" />
    </HBox>
    <Label fx:id="welcomeLabel" text="Welcome, Administrator!" style="-fx-font-size: 24px;"/>

    <Button text="View Assessed Disasters" onAction="#viewAdminDisasters" />
    <Button text="Edit Users"/>

</VBox>

